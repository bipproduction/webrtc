    "elysia": "^1.3.5",
    "lucide-react": "^0.525.0",
    "react": "^19",
    "react-dom": "^19",
    "react-router-dom": "^7.6.3",
    "swr": "^2.3.4",
    "uuid": "^11.1.0",
    "valtio": "^2.1.5"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest"
  }
